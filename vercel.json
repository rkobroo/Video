
{
  "version": 2,
  "builds": [
    {
      "src": "main.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "50mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "main.py"
    }
  ],
  "env": {
    "PYTHONPATH": ".vercel/source",
    "SESSION_SECRET": "vercel-production-secret-key-change-this"
  },
  "functions": {
    "main.py": {
      "memory": 1024,
      "maxDuration": 60
    }
  }
}
